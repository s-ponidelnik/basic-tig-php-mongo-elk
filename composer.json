    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3.0",
        "yiisoft/yii2": "~2.0.36",
        "symfony/dotenv": "^5.1",
        "yiisoft/yii2-redis": "~2.0.0",
        "guzzlehttp/guzzle": "^7.0",
        "ext-openssl": "*",
        "ext-json": "*",
        "bower-asset/noty": "^2.3",
        "dmstr/yii2-adminlte-asset": "^2.6",
        "kartik-v/yii2-date-range": "^1.7.0",
        "pmipay/yii2-merchant": "dev-stage",
        "pmipay/yii2-pm-cashmaal": "dev-stage",
        "pmipay/yii2-pm-p4f-brazil": "dev-stage",
        "pmipay/yii2-pm-directa24": "dev-stage",
        "pmipay/yii2-pm-gopaypro": "dev-stage",
        "pmipay/yii2-pm-vouchers": "dev-stage",
        "pmipay/yii2-pm-transact365": "dev-stage",
        "pmipay/yii2-pm-irgate": "dev-stage",
        "pmipay/yii2-pm-paytm": "dev-stage",
        "pmipay/yii2-pm-p2p": "dev-stage",
        "pmipay/yii2-pm-payconnect": "dev-stage",
        "pmipay/yii2-pm-pagsmile": "dev-stage",
        "pmipay/yii2-pm-interkassa": "dev-stage",
        "pmipay/yii2-pm-mpsa": "dev-stage",
        "pmipay/yii2-pm-zenpay-paylogic": "dev-stage",
        "pmipay/yii2-pm-zer0pay": "dev-stage",
        "pmipay/yii2-pm-zigowallet": "dev-stage",
        "pmipay/yii2-pm-paystack": "dev-stage",
        "pmipay/yii2-pm-zenpay-payfair": "dev-stage",
        "pmipay/yii2-pm-opay": "dev-stage",
        "zircote/swagger-php": "^3.1",
        "bower-asset/chart-js": "^2.9",
        "yiisoft/yii2-elasticsearch": "^2.1",
        "yiisoft/yii2-swiftmailer": "^2.1",
        "kartik-v/yii2-widget-select2": "dev-master",
        "kotchuprik/yii2-sortable-widgets": "^2.1",
        "kartik-v/yii2-widget-timepicker": "^1.0",
        "himiklab/yii2-recaptcha-widget": "*",
        "kartik-v/yii2-widget-datetimepicker": "*",
        "kartik-v/yii2-widget-datepicker": "^1.4",
        "kartik-v/yii2-widget-fileinput": "^1.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "yiisoft/yii2-debug": "~2.0.0",
        "codeception/codeception": "^4.1",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0",
        "codeception/module-yii2": "^1.1",
        "squizlabs/php_codesniffer": "3.*"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "scripts": {
        "set-perms": [
            "chmod 777 api/runtime",
            "chmod 777 api/web/assets",
            "chmod 777 backend/runtime",
            "chmod 777 backend/web/assets",
            "chmod 777 backend/web/files",
            "chmod 777 console/runtime"
        ],
        "phpcs": [
            "./vendor/bin/phpcs --standard=psr12-ruleset.xml --colors -p"
        ],
        "phpcbf": [
            "./vendor/bin/phpcbf --standard=psr12-ruleset.xml"
        ],
        "up": [
            "composer update pmipay/yii2-pm-opay pmipay/yii2-pm-zenpay-payfair pmipay/yii2-pm-paystack pmipay/yii2-pm-zigowallet pmipay/yii2-pm-zer0pay pmipay/yii2-pm-zenpay-paylogic pmipay/yii2-pm-pagsmile pmipay/yii2-pm-mpsa pmipay/yii2-pm-interkassa pmipay/yii2-pm-payconnect pmipay/yii2-pm-p2p pmipay/yii2-pm-paytm pmipay/yii2-pm-irgate pmipay/yii2-pm-transact365 pmipay/yii2-pm-vouchers pmipay/yii2-pm-gopaypro pmipay/yii2-pm-directa24 pmipay/yii2-pm-p4f-brazil pmipay/yii2-pm-cashmaal pmipay/yii2-merchant"
        ],
        "release": [
            "git pull && ./yii migrate/up --interactive=0 && composer install && ./migrate-all.sh && ./yii cache/flush-all"
        ],
        "test-migrate": [
            "./console/test/yii migrate && ./console/test/yii migrate --migrationPath=@console/migrations/"
        ]
    }
}
